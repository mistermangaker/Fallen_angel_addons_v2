	    <techLevel>Neolithic</techLevel>
	    <label>merp Outfit</label>
	    <graphicData>
	      <texPath>Things/Apparel/Swe_merp_outfit/dress4</texPath>
	      <graphicClass>Graphic_Single</graphicClass>
	    
	    </graphicData>
		<thingClass>Fallen_angel.NoColorApparel</thingClass>
	    <costStuffCount>80</costStuffCount>
		<stuffCategories>
		<li>Fabric</li>
		<li>Leathery</li>
